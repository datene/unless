<div id="footer">
  <div class="social">
    <% data.general.first.social.each do |social_media_item| %>
      <% link_to social_media_item.last do %>
        <i class="fa fa-<%= social_media_item.first %>"></i>
      <% end %>
    <% end %>
  </div>
  <a href="mailto:<%= data.general.first.email %>?SUBJECT=<%= data.general.first.email_subject %>" class="email-link">
    <%= data.general.first.email %>
  </a>
</div>

